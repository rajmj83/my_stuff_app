<% @the_web_page_title = "Show category: #{@category.name}" %>

<%= link_to("<< Back to List", {:action => 'list'}, :class => 'back-link') %>

<div class="subject show">
  <h2>Show Category</h2>
   <table summary="Category detail view">
    <tr>
      <th>Name</th>
      <td><%= @category.name %></td>
    </tr>
    <tr>
      <th>Symbol</th>
      <td><%= @category.symbol%></td>
    </tr>
    <tr>
      <th>Created</th>
      <td><%= @category.created_at %></td>
    </tr>
    <tr>
      <th>Updated</th>
      <td><%= @category.updated_at %></td>
    </tr>
  </table>
  
</div>

<hr />
<%= params.inspect %>
